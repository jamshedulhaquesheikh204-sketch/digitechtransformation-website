{
  "version": 2,
  "name": "digitech-transformation-junaid-ul-haque-sheikh-0001",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "images/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "dest": "/images/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/index",
      "dest": "/index.html"
    }
  ],
  "github": {
    "enabled": true,
    "silent": true
  }
}
